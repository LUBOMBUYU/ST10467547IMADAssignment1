<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AfternoonMeal">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="1022dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="VisualLintBounds,TextSizeCheck">

        <ImageView
            android:id="@+id/Background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/lunch_background"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/AfternoonMenu"
            android:layout_width="1010dp"
            android:layout_height="86dp"
            android:layout_marginTop="4dp"
            android:background="@color/black"
            android:gravity="center"
            android:padding="16dp"
            android:text="Afternoon Menu"
            android:textColor="@android:color/white"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/Background"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/BlackBeanQuesadillasImage"
            android:layout_width="300dp"
            android:layout_height="200dp"
            android:layout_marginStart="196dp"
            android:layout_marginTop="96dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/black_bean_quesadillas"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="75dp"
            android:layout_marginTop="7dp"
            android:background="@color/light_grey"
            android:fontFamily="@font/comfortaa_variablefont_wght"
            android:text="Black Bean\nQuesadillas"
            android:textColor="#33691E"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="@+id/BlackBeanQuesadillasImage"
            app:layout_constraintTop_toBottomOf="@+id/BlackBeanQuesadillasImage"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/StirFriedTofuWithRiceImage"
            android:layout_width="277dp"
            android:layout_height="201dp"
            android:layout_marginTop="92dp"
            android:layout_marginEnd="204dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/stir_fried_tofu_with_rice"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/StirFriedTofuWithRiceText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="64dp"
            android:background="@color/light_grey"
            android:fontFamily="@font/comfortaa_variablefont_wght"
            android:text="Stir Fried Tofu\nwith Rice"
            android:textColor="#33691E"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/StirFriedTofuWithRiceImage"
            app:layout_constraintTop_toBottomOf="@+id/StirFriedTofuWithRiceImage"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/WholeWheatPastawithPestoCherryTomatoesImage"
            android:layout_width="191dp"
            android:layout_height="196dp"
            android:layout_marginTop="396dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.275"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/whole_wheat_pasta_with_pesto___cherry_tomatoes"
            tools:ignore="ContentDescription,ImageContrastCheck" />

        <TextView
            android:id="@+id/WholeWheatPastawithPestoCherryTomatoesText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="@color/light_grey"
            android:fontFamily="@font/comfortaa_variablefont_wght"
            android:text="Whole Wheat Pasta\nwith Pesto \nCherry Tomatoes"
            android:textColor="#33691E"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.279"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/WholeWheatPastawithPestoCherryTomatoesImage"
            tools:ignore="HardcodedText,VisualLintOverlap" />

        <TextView
            android:id="@+id/VeganBbqRceandBeansBowlText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/light_grey"
            android:fontFamily="@font/comfortaa_variablefont_wght"
            android:text="Vegan Bbq Rice\nand Beans Bowl "
            android:textColor="#33691E"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/VeganBbqRceandBeansBowlmage"
            app:layout_constraintTop_toBottomOf="@+id/VeganBbqRceandBeansBowlmage"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/wholeWheatPastawithPestoCherryTomatoesText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="@color/light_grey"
            android:fontFamily="@font/comfortaa_variablefont_wght"
            android:text="Whole Wheat Pasta\nwith Pesto \nCherry Tomatoes"
            android:textColor="#33691E"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.279"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/WholeWheatPastawithPestoCherryTomatoesImage"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/VeganBbqRceandBeansBowlmage"
            android:layout_width="168dp"
            android:layout_height="222dp"
            android:layout_marginTop="384dp"
            android:layout_marginEnd="272dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/vegan_bbq_rice_and_beans_bowl"
            tools:ignore="ContentDescription" />

        <Button
            android:id="@+id/btnBack3"
            android:layout_width="215dp"
            android:layout_height="84dp"
            android:layout_marginBottom="152dp"
            android:text="Back"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="HardcodedText" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>